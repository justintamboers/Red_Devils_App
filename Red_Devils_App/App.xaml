<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Red_Devils_App"
             xmlns:converters="clr-namespace:Red_Devils_App.Converter"
             x:Class="Red_Devils_App.App">
    <Application.Resources>
        <ResourceDictionary>
            <x:String x:Key="fallbackImageUrl">unkown.png</x:String>
            <converters:AgeConverter x:Key="AgeConverter"></converters:AgeConverter>
                <converters:PositionConverter x:Key="PositionConverter"></converters:PositionConverter> 
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
